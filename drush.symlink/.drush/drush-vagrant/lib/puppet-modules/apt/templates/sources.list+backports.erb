# backports
<% if (lsbdistcodename == "sid" || lsbdistcodename == "unstable") -%>
# There are no backports for for <%= lsbdistcodename %>
<% else -%>
deb http://backports.debian.org/debian-backports <%= lsbdistcodename %>-backports main
<% end -%>

